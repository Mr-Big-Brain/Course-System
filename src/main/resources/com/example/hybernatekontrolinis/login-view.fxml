   <children>
      <Label layoutX="79.0" layoutY="49.0" text="Login:" />
      <Label layoutX="59.0" layoutY="98.0" text="Password:" />
      <Button fx:id="loginButton" layoutX="223.0" layoutY="141.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <Button fx:id="registerButton" layoutX="82.0" layoutY="141.0" mnemonicParsing="false" onAction="#register" text="Register" />
      <TextField fx:id="loginTextField" layoutX="120.0" layoutY="45.0" />
      <TextField fx:id="passwordTextField" layoutX="120.0" layoutY="94.0" />
   </children>
</AnchorPane>
