<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="486.0" prefWidth="799.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hybernatekontrolinis.JavaFxControllers.ManagePeopleController">
   <children>
      <TableView fx:id="defaultUsersTable" layoutX="58.0" layoutY="92.0" prefHeight="200.0" prefWidth="292.0">
        <columns>
          <TableColumn fx:id="defaultLoginColumn" prefWidth="92.0" text="Login" />
          <TableColumn fx:id="defaultNameColumn" prefWidth="96.0" text="Name" />
            <TableColumn fx:id="defaultSurnameColumn" prefWidth="103.0" text="Surname" />
        </columns>
      </TableView>
      <TableView fx:id="specificUsersTable" layoutX="459.0" layoutY="92.0" prefHeight="200.0" prefWidth="292.0">
        <columns>
          <TableColumn fx:id="specificLoginColumn" prefWidth="89.0" text="Login" />
          <TableColumn fx:id="specificNameColumn" prefWidth="106.0" text="Name" />
            <TableColumn fx:id="specificSurnameColumn" prefWidth="96.0" text="Surname" />
        </columns>
      </TableView>
      <Label fx:id="defaultUsersLabel" layoutX="58.0" layoutY="62.0" text="Label" />
      <Label fx:id="specificUsersLabel" layoutX="459.0" layoutY="62.0" text="Label" />
      <Button fx:id="fromSpecificToDefaultButton" layoutX="374.0" layoutY="180.0" mnemonicParsing="false" onAction="#removeFromSpecificToDefault" prefHeight="25.0" prefWidth="52.0" text="&lt;-" />
      <Button fx:id="fromDefaultToSpecificButton" layoutX="374.0" layoutY="128.0" mnemonicParsing="false" onAction="#removeFromDefaultToSpecific" prefHeight="25.0" prefWidth="52.0" text="-&gt;" />
      <Button fx:id="backToCoursesButton" layoutX="60.0" layoutY="381.0" mnemonicParsing="false" onAction="#goBackToCourses" text="Back to courses" />
   </children>
</AnchorPane>
